<Window x:Class="_6TI_VandervoortAlexandre_Act4_MultiProjet.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_6TI_VandervoortAlexandre_Act4_MultiProjet"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="6*"/>
        </Grid.RowDefinitions>

        <Button Content="Accueil" x:Name="Accueil"/>
        <Button Content="Matching Game" x:Name="Nav1" Grid.Column="1"/>
        <Button Content="Trinome" x:Name="Nav2" Grid.Column="2"/>
        <Button Content="Chalet" x:Name="Nav3" Grid.Column="3"/>
        

        <Frame Grid.Row="1" Grid.ColumnSpan="4" x:Name="MainFrame" NavigationUIVisibility="Hidden">
            
        </Frame>
    </Grid>
</Window>
